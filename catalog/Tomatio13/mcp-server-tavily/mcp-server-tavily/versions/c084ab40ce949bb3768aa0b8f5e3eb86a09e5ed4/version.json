  "manifest": {
    "id": "mcp-server-tavily",
    "fullId": "Tomatio13/mcp-server-tavily/mcp-server-tavily",
    "repo": {
      "provider": "github.com",
      "owner": "Tomatio13",
      "repo": "mcp-server-tavily",
      "branch": "main",
      "url": "https://github.com/Tomatio13/mcp-server-tavily"
    },
    "config": {
      "configValues": [
        {
          "title": "tavily-api-key",
          "description": null,
          "default": null,
          "required": false,
          "fields": [
            {
              "type": "environment",
              "key": "TAVILY_API_KEY"
            }
          ]
        }
      ]
    },
    "subdirectory": "/",
    "title": "tavily-search MCP server",
    "description": "This server uses the Tavily API to perform searches based on specified queries."
  },
  "manifestHash": "3bea32a7e94c774fef796bd11257f453",
  "createdAt": "2025-04-15T07:16:17.649Z",
  "builder": [
    {
      "type": "nixpacks",
      "plan": {
        "providers": [],
        "buildImage": "ghcr.io/railwayapp/nixpacks:ubuntu-1742861060",
        "variables": {
          "NIXPACKS_METADATA": "python",
          "NIXPACKS_UV_VERSION": "0.4.30",
          "PIP_DEFAULT_TIMEOUT": "100",
          "PIP_DISABLE_PIP_VERSION_CHECK": "1",
          "PIP_NO_CACHE_DIR": "1",
          "PYTHONDONTWRITEBYTECODE": "1",
          "PYTHONFAULTHANDLER": "1",
          "PYTHONHASHSEED": "random",
          "PYTHONUNBUFFERED": "1",
          "UV_PROJECT_ENVIRONMENT": "/opt/venv"
        },
        "phases": {
          "install": {
            "dependsOn": [
              "setup"
            ],
            "cmds": [
              "python -m venv --copies /opt/venv && . /opt/venv/bin/activate && pip install uv==$NIXPACKS_UV_VERSION && uv sync --no-dev --frozen"
            ],
            "cacheDirectories": [
              "/root/.cache/uv"
            ],
            "paths": [
              "/opt/venv/bin"
            ]
          },
          "setup": {
            "nixPkgs": [
              "python313",
              "gcc"
            ],
            "nixLibs": [
              "zlib",
              "stdenv.cc.cc.lib"
            ],
            "nixOverlays": [],
            "nixpkgsArchive": "bc8f8d1be58e8c8383e683a06e1e1e57893fff87"
          }
        },
        "start": {
          "cmd": "python -m tavily-search"
        }
      }
    }
  ]
}