        "variables": {
          "NIXPACKS_METADATA": "python",
          "NIXPACKS_UV_VERSION": "0.4.30",
          "PIP_DEFAULT_TIMEOUT": "100",
          "PIP_DISABLE_PIP_VERSION_CHECK": "1",
          "PIP_NO_CACHE_DIR": "1",
          "PYTHONDONTWRITEBYTECODE": "1",
          "PYTHONFAULTHANDLER": "1",
          "PYTHONHASHSEED": "random",
          "PYTHONUNBUFFERED": "1",
