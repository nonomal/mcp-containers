        },
        "phases": {
          "install": {
            "dependsOn": [
              "setup"
            ],
            "cmds": [
              "python -m venv --copies /opt/venv && . /opt/venv/bin/activate && pip install uv==$NIXPACKS_UV_VERSION && uv sync --no-dev --frozen"
            ],
            "cacheDirectories": [
              "/root/.cache/uv"
            ],
            "paths": [
              "/opt/venv/bin"
            ]
          },
          "setup": {
            "nixPkgs": [
              "python311",
              "ffmpeg-headless",
              "gcc"
            ],
