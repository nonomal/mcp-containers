            "cacheDirectories": [
              "/root/.cache/uv"
            ],
            "paths": [
              "/opt/venv/bin"
            ]
          },
          "setup": {
            "nixPkgs": [
              "python311",
              "gcc"
            ],
            "nixLibs": [
              "zlib",
