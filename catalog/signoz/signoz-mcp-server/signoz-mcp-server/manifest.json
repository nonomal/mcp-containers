{
  "id": "signoz-mcp-server",
  "fullId": "signoz/signoz-mcp-server/signoz-mcp-server",
  "repo": {
    "provider": "github.com",
    "owner": "signoz",
    "repo": "signoz-mcp-server",
    "branch": "main",
    "url": "https://github.com/signoz/signoz-mcp-server"
  },
  "config": {
    "argumentsTemplate": null,
    "configValues": [
      {
        "title": "signoz-url",
        "description": "URL of the SigNoz instance",
        "default": null,
        "required": false,
        "fields": [
          {
            "type": "environment",
            "key": "SIGNOZ_URL"
          }
        ]
      },
      {
        "title": "signoz-api-key",
        "description": "API key to connect to SigNoz",
        "default": null,
        "required": false,
        "fields": [
          {
            "type": "environment",
            "key": "SIGNOZ_API_KEY"
          }
        ]
      }
    ]
  },
  "subdirectory": "/",
  "title": "SigNoz MCP Server",
  "description": "Connect to SigNoz trough MCP. This server enables natural language queries for metrics, alerts, dashboards, and service performance data."
}